        OLHA_A_MERDA = ME_DA_UM - ME_DA_DOIS;
    QUE NAO VAI DAR O QUE? (BORA_CRECE_CARAI == '*')
        OLHA_A_MERDA = ME_DA_UM * ME_DA_DOIS;
